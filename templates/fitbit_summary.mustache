{{#activities}}
    <div class="activity-header" data-toggle="collapse" data-target="#activity{{logId}}">
        <div class="act act-name">{{name}}</div>
        <div class="act act-date">{{startTime}}</div>
        <div class="act act-duration">Duration: {{duration}}</div>
        <div class="act act-steps">Steps: {{steps}}</div>
        <div class="act act-calories">Calories: {{calories}}</div>
        <div class="act act-heart">Heart Rate: {{heartRate}}</div>
    </div>
    <div class="collapse activity-collapse" id="activity{{logId}}" data-tcx="{{tcxLink}}" data-logId="{{logId}}">
        <div class="activity-container">
            <div id="fitbit-map-{{actNum}}" class="fitbit-map map-hidden"></div>
            <div id="fitbit-map-{{actNum}}-data" class="fitbit-map-data map-hidden"></div>
        </div>
    </div>
{{/activities}}

